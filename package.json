{
  "name": "posthtml-attrs-sorter",
  "description": "The sorting attribute of the tag based on the given order",
  "version": "0.1.4",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "repository": "mrmlnc/posthtml-attrs-sorter",
  "license": "MIT",
  "keywords": [
    "posthtml",
    "html",
    "posthtml-plugin",
    "attrs",
    "attributes",
    "sorter",
    "sort"
  ],
  "dependencies": {},
  "devDependencies": {
    "mocha": "^3.0.1",
    "posthtml": "^0.9.0",
    "xo": "^0.16.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "test": "xo && mocha"
  },
  "files": [
    "index.js"
  ],
  "xo": {
    "space": 2,
    "rules": {
      "arrow-parens": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ]
    },
    "envs": [
      "mocha",
      "node"
    ]
  }
}
